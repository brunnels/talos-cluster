{
  "Servers": {
    "1": {
      "Name": {{ index . "dbname" | quote }},
      "Group": "Servers",
      "Host": {{ index . "host" | quote }},
      "Port": {{ index . "port" }},
      "MaintenanceDB": {{ index . "dbname" | quote }},
      "Username": {{ index . "dbname" | quote }},
      "SSLMode": "prefer",
      "SSLCert": "<STORAGE_DIR>/.postgresql/postgresql.crt",
      "SSLKey": "<STORAGE_DIR>/.postgresql/postgresql.key",
      "SSLCompression": 0,
      "Timeout": 10,
      "UseSSHTunnel": 0,
      "TunnelPort": "22",
      "TunnelAuthentication": 0
    }
  }
}
